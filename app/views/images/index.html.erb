      <p><a href="#">Link</a></p>
      <p><a href="#">Link</a></p>
      <p><a href="#">Link</a></p>
      <%= link_to 'New Image', new_image_path %>
    </div>
    <div class="col-sm-7 text-left"> 
    <table>
      <tbody>
        <% @images.each do |image| %>
        <div class="show-list-pic">
          <%= image.title %></br>
          景色名: <%= image.scenery_name %></br>
          景色の住所: <%= image.scenery_address %>
          <p><%= image.description %></p>
          <%= image_tag image.scenery_picture, :width =>'500', :height =>'auto' %><br/>
          <%= link_to 'Show', image %>
          <%= link_to 'Edit', edit_image_path(image) %>
          <%= link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' } %>
        </div>
        <hr>
